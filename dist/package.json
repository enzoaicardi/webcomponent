{
    "name": "@enzoaicardi/webcomponent",
    "type": "module",
    "version": "2.3.0",
    "description": "A customElements abstraction layer to create web components that can be rendered on both client and server sides",
    "homepage": "https://github.com/enzoaicardi/webcomponent",
    "bugs": "https://github.com/enzoaicardi/webcomponent/issues",
    "author": {
        "name": "Enzo Aicardi",
        "url": "https://enzo.aicardi.pro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/enzoaicardi/webcomponent"
    },
    "main": "./esm/client.js",
    "module": "./esm/client.js",
    "browser": "./esm/client.js",
    "types": "./client.d.ts",
    "exports": {
        "./client": {
            "types": "./client.d.ts",
            "require": "./cjs/client.cjs",
            "import": "./esm/client.js",
            "browser": "./esm/client.js",
            "default": "./esm/client.js"
        },
        "./server": {
            "types": "./server.d.ts",
            "require": "./cjs/server.cjs",
            "import": "./esm/server.js",
            "browser": "./esm/server.js",
            "default": "./esm/server.js"
        },
        "./client/esm": {
            "types": "./client.d.ts",
            "default": "./esm/client.js"
        },
        "./client/cjs": {
            "types": "./client.d.ts",
            "default": "./cjs/client.cjs"
        },
        "./client/umd": {
            "types": "./client.d.ts",
            "default": "./umd/client.js"
        },
        "./client/iife": {
            "types": "./client.d.ts",
            "default": "./iife/client.js"
        },
        "./server/esm": {
            "types": "./server.d.ts",
            "default": "./esm/server.js"
        },
        "./server/cjs": {
            "types": "./server.d.ts",
            "default": "./cjs/server.cjs"
        },
        "./server/umd": {
            "types": "./server.d.ts",
            "default": "./umd/server.js"
        },
        "./server/iife": {
            "types": "./server.d.ts",
            "default": "./iife/server.js"
        }
    },
    "keywords": [
        "typescript",
        "javascript",
        "webcomponent",
        "customElements",
        "ssr",
        "csr"
    ],
    "scripts": {
        "test": "echo \"No test specified\""
    },
    "license": "GPL-3.0-only"
}
